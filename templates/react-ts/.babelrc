{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "modules": false,
        "corejs": { "version": 3, "proposals": true },
        "debug": false,
        "targets": {
          "esmodules": true
        }
      }
    ],
    "@babel/typescript",

    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": ["react-hot-loader/babel"]
}
