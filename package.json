    "inquirer": "^8.2.2",
    "yargs-parser": "^21.0.1"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.4",
    "@types/inquirer": "^8.2.0",
    "@types/node": "^17.0.23",
    "@types/shelljs": "^0.8.11",
    "@types/yargs-parser": "^21.0.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
